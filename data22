import pandas as pd
from datetime import datetime

def parse_timestamp(timestamp):
    return datetime.strptime(timestamp, "%Y-%m-%dT%H:%M:%S%z")

def calculate_time_difference(file_path):
    # Read the Excel file
    df = pd.read_excel(file_path, usecols=["created", "updated"], nrows=2)
    
    # Parse timestamps
    created_time = parse_timestamp(df.loc[0, "created"])
    updated_time = parse_timestamp(df.loc[0, "updated"])
    
    # Calculate difference
    time_difference = updated_time - created_time
    
    return time_difference

# Example usage
file_path = "your_excel_file.xlsx"  # Change to your actual file path
diff = calculate_time_difference(file_path)
print("Time Difference:", diff)
