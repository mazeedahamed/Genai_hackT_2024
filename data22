unmatched_list = []

for data in extracted_data:
    # Convert data values to empty strings if None or blank
    data_type = str(data.get('type') or '').strip().lower()
    data_name = str(data.get('name') or '').strip().lower()
    data_country = str(data.get('country') or '').strip().lower()

    # Prepare DataFrame columns with consistent formatting and NaN handling
    df_type = df['type'].fillna('').astype(str).str.strip().str.lower()
    df_name = df['name'].fillna('').astype(str).str.strip().str.lower()
    df_country = df['country'].fillna('').astype(str).str.strip().str.lower()

    # Create the match condition
    match_condition = (
        (df_type == data_type) &
        (df_name == data_name) &
        (df_country == data_country)
    )
    
    # Check for unmatched rows
    if not match_condition.any():
        unmatched_list.append({
            'data_type': data_type,
            'data_name': data_name,
            'data_country': data_country
        })

# Print unmatched entries
if unmatched_list:
    print("\n❌ Unmatched List:")
    for item in unmatched_list:
        print(item)
else:
    print("\n✅ All data matched successfully.")
